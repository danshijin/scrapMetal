<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smm.scrapMetal.dao.ICategoryDao">

<select id="getCategorys" resultType="com.smm.scrapMetal.domain.Category">	
	SELECT id,modID,NAME,listOrder,STATUS FROM ch_category 
	WHERE modID=1

</select>


</mapper>